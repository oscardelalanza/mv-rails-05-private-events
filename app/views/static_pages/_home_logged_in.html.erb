<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <%= render 'shared/user_info' %>
    </section>
    <section class="event_form">
      <%= render 'shared/event_form' %>
    </section>
  </aside>
  <div class="col-md-8">
    <div>
      <h3>Previous Events</h3>
      <%= render @previous %>
      <%= will_paginate @previous %>
    </div>
    <div>
      <h3>Next Events</h3>
      <%= render @next %>
      <%= will_paginate @previous %>
    </div>
  </div>
</div>
